        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".InfromasiActivity" android:parentActivityName=".InfromasiActivity"></activity>
        <activity android:name=".DetailDataActivity" android:parentActivityName=".DetailDataActivity"/>
        <activity android:name=".UpdateDataActivity" android:parentActivityName=".UpdateDataActivity"/>
        <activity android:name=".InputDataActivity" android:parentActivityName=".InputDataActivity"/>
        <activity android:name=".LihatDataActivity" android:parentActivityName=".LihatDataActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>